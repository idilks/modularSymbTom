# Copy this file to .env and fill in your actual values
# .env is already gitignored so your secrets stay local

# Hugging Face token for model access
# Get yours from: https://huggingface.co/settings/tokens
HF_TOKEN=your_hf_token_here

# Wandb API key for experiment tracking  
# Get yours from: https://wandb.ai/authorize
WANDB_API_KEY=your_wandb_key_here

# Optional: Custom cache directory for models
# Defaults to HPC path if not set
HF_HOME=/path/to/your/cache